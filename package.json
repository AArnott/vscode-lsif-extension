{
    "name": "vscode-sip-extension",
    "displayName": "vscode-sip-extension",
    "description": "",
    "version": "0.0.1",
    "publisher": "MS",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "enableProposedApi": true,
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.6",
        "vscode": "^1.1.21",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42",
        "@types/semver": "^5.5.0"
    },
    "dependencies": {
        "vscode-languageclient": "^5.1.0",
        "vscode-languageserver-protocol": "^3.13.0",
        "semver": "^5.6.0"
    }
}
